<h1>Iniciar Sessão</h1>

<%= form_for :session, url: login_path do |f| %>
    <div class="field">
      <%= f.label :email %><br />
      <%= f.email_field :email, autofocus: true %>
    </div>

    <div class="field">
      <%= f.label :password %><br />
      <%= f.password_field :password, autocomplete: "off" %>
    </div>

    <div class="actions">
      <%= f.submit "Log in" %>
    </div>

  <!-- <%= link_to "Login com Facebook", "/auth/facebook" %> -->
